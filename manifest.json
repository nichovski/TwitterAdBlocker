{
    "manifest_version": 3,
    "name": "Twitter ad blocker",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["https://twitter.com/*"],
        "js": ["remove-ads.js"],
        "run_at": "document_end",
        "all_frames": false
      }
    ],
    "host_permissions": [
      "https://twitter.com/*"
    ]
  }
  